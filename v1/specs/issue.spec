attributes:
- description: Data contains additional data. The value depends on the issuer type.
  exposed: true
  filterable: true
  format: free
  name: data
  orderable: true
  stored: true
  type: string
- description: Metadata contains various additional information. Meaning depends on
    the realm.
  exposed: true
  name: metadata
  orderable: true
  subtype: metadata
  type: external
- autogenerated: true
  description: Token is the token to use for the registration.
  exposed: true
  format: free
  name: token
  read_only: true
  type: string
- allowed_chars: ^([0-9]+h[0-9]+m[0-9]+s|[0-9]+m[0-9]+s|[0-9]+m[0-9]+s|[0-9]+h[0-9]+s|[0-9]+h[0-9]+m|[0-9]+s|[0-9]+h|[0-9]+m)$
  default_value: 24h
  description: Validity configures the max validity time for a token. If it is bigger
    than the configured max validity, it will be capped.
  exposed: true
  filterable: true
  format: free
  name: validity
  orderable: true
  stored: true
  type: string
model:
  description: This API issues a new token according to given data.
  entity_name: Issue
  extends:
  - '@realm'
  package: midgard
  resource_name: issue
  rest_name: issue

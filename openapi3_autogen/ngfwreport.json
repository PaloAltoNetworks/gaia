{
  "openapi": "3.0.3",
  "components": {
    "schemas": {
      "ngfwreport": {
        "description": "Defines a Next-Generation Firewall (NGFW) report.",
        "properties": {
          "ID": {
            "description": "Identifier of the object.",
            "readOnly": true,
            "type": "string"
          },
          "instanceID": {
            "description": "Cloud NGFW instance for generated report.",
            "example": 12345678,
            "type": "string"
          },
          "namespace": {
            "description": "Namespace tag attached to an entity.",
            "readOnly": true,
            "type": "string"
          },
          "reportType": {
            "default": [
              "NetworkingReport"
            ],
            "description": "Type of Cloud NGFW report stored.",
            "enum": [
              "NetworkingReport",
              "SecurityReport"
            ]
          },
          "timestamp": {
            "description": "Date of the report.",
            "example": "2018-06-14T23:10:46.420397985Z",
            "format": "date-time",
            "type": "string"
          },
          "topApplications": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top applications used for generated networking report.",
            "example": [
              {
                "app1": 200
              },
              {
                "app2": 100
              }
            ],
            "type": "object"
          },
          "topAttackCategories": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top attack categories used for generated security report.",
            "example": [
              {
                "Phishing": 200
              },
              {
                "Malware": 100
              }
            ],
            "type": "object"
          },
          "topAttackedApplications": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top applications attacked used for generated security report.",
            "example": [
              {
                "app1": 200
              },
              {
                "app2": 100
              }
            ],
            "type": "object"
          },
          "topDestinationVictims": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top destination IPs for victims used for generated security report.",
            "example": [
              {
                "127.0.0.1": 200
              },
              {
                "0.0.0.0": 100
              }
            ],
            "type": "object"
          },
          "topDestinations": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top IP destinations for generated networking report.",
            "example": [
              {
                "127.0.0.1": 200
              },
              {
                "0.0.0.0": 100
              }
            ],
            "type": "object"
          },
          "topDetectedAttacks": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top detected attacks used for generated security report.",
            "example": [
              {
                "Log4j": 200
              },
              {
                "TrojanX": 100
              }
            ],
            "type": "object"
          },
          "topSourceAttackers": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top source IPs in attacks used for generated security report.",
            "example": [
              {
                "127.0.0.1": 200
              },
              {
                "0.0.0.0": 100
              }
            ],
            "type": "object"
          },
          "topSources": {
            "additionalProperties": {
              "type": "integer"
            },
            "description": "Top IP sources for generated networking report.",
            "example": [
              {
                "127.0.0.1": 200
              },
              {
                "0.0.0.0": 100
              }
            ],
            "type": "object"
          },
          "totalEgress": {
            "description": "Total egress traffic in bytes for generated networking report.",
            "example": 1000000,
            "type": "integer"
          },
          "totalIngress": {
            "description": "Total ingress traffic in bytes for generated networking report.",
            "example": 1000000,
            "type": "integer"
          }
        },
        "required": [
          "instanceID",
          "reportType",
          "timestamp"
        ],
        "type": "object"
      }
    }
  },

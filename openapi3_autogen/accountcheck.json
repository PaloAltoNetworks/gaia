{
  "openapi": "3.0.3",
  "components": {
    "schemas": {
      "accountcheck": {
        "description": "Validates the password for an account.",
        "properties": {
          "ID": {
            "description": "ID of the account if validated.",
            "readOnly": true,
            "type": "string"
          },
          "OTP": {
            "description": "eventual otp.",
            "type": "string"
          },
          "email": {
            "description": "email of the account if validated.",
            "readOnly": true,
            "type": "string"
          },
          "handle": {
            "description": "Account name of email.",
            "example": "acme",
            "type": "string"
          },
          "name": {
            "description": "name of the account.",
            "readOnly": true,
            "type": "string"
          },
          "password": {
            "description": "provided password.",
            "example": "S3cr3t#",
            "type": "string"
          }
        },
        "required": [
          "handle",
          "password"
        ],
        "type": "object"
      }
    }
  },

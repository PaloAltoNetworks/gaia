{
  "openapi": "3.0.3",
  "components": {
    "schemas": {
      "sshcertificate": {
        "description": "Internal api to deliver SSH certificate.",
        "properties": {
          "certificate": {
            "description": "Contains the signed SSH certificate in OpenSSH format.",
            "readOnly": true,
            "type": "string"
          },
          "extensions": {
            "additionalProperties": {
              "type": "string"
            },
            "description": "List of extensions to set in the SSH certificate.",
            "type": "object"
          },
          "options": {
            "additionalProperties": {
              "type": "string"
            },
            "description": "List of options to set in the SSH certificate.",
            "type": "object"
          },
          "principals": {
            "description": "List of principals to set in the SSH certificate.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "publicKey": {
            "description": "Contains the public key to sign in OpenSSH format.",
            "example": "ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAABAQCytT my key",
            "type": "string"
          },
          "signerID": {
            "description": "The identifier of the CA to use to sign the certificate.",
            "example": "xxx-xxx-xxx-xxx",
            "type": "string"
          },
          "type": {
            "default": "User",
            "description": "Type of SSH certificate.",
            "enum": [
              "User",
              "Host"
            ]
          },
          "validity": {
            "default": "1h",
            "description": "Set the validity of the SSH certificate.",
            "type": "string"
          }
        },
        "required": [
          "publicKey",
          "signerID"
        ],
        "type": "object"
      }
    }
  },
  "info": {
    "contact": {
      "email": "dev@aporeto.com",
      "name": "Aporeto Inc.",
      "url": "go.aporeto.io/api"
    },
    "license": {
      "name": "TODO"
    },
    "termsOfService": "https://localhost/TODO",
    "title": "sshcertificate",
    "version": "1.0"
  },
  "paths": {
    "/sshcertificates": {
      "post": {
        "description": "Creates a new SSH certificate.",
        "operationId": "create-a-new-sshcertificate",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/sshcertificate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/sshcertificate"
                }
              }
            },
            "description": "n/a"
          }
        },
        "tags": [
          "internal/ssh",
          "barret"
        ]
      }
    }
  },
  "tags": [
    {
      "description": "This tag is for group 'internal/ssh'",
      "name": "internal/ssh"
    },
    {
      "description": "This tag is for package 'barret'",
      "name": "barret"
    }
  ]
}

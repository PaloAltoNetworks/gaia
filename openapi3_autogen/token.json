          },
          "tags": {
            "description": "Tags includes a list of tags that must be added to the token.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "token": {
            "description": "Token contains the generated token.",
            "readOnly": true,
            "type": "string"
          },
          "validity": {
            "description": "Validity contains the token validity duration.",
            "type": "string"
          }
        },
        "required": [

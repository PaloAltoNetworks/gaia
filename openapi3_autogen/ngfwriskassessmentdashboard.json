        "operationId": "create-a-new-ngfwriskassessmentdashboard",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ngfwriskassessmentdashboard"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ngfwriskassessmentdashboard"
                }
              }
            },
            "description": "n/a"
          }
        },
        "tags": [
          "placeholder",
          "placeholder"
        ]
      }
    },
    "/ngfwriskassessmentdashboards/{id}": {
      "get": {
        "description": "Retrieves NGFW risk assessment dashboard metrics for the specified cloud account and time filter.",
        "operationId": "get-ngfwriskassessmentdashboard-by-ID",
        "parameters": [
          {
            "description": "Also retrieve the objects that propagate down.",
            "in": "query",
            "name": "propagated",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ngfwriskassessmentdashboard"
                }
              }
            },
            "description": "n/a"
          }
        },
        "tags": [
          "placeholder",
          "placeholder"
        ]
      },
      "parameters": [
        {
          "in": "path",
          "name": "id",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ]
    }
  },

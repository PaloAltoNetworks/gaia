    "/cloudgraphs": {
      "post": {
        "description": "Creates a cloud dependency graph based on ingested data and the required\nparameters.",
        "operationId": "create-a-new-cloudgraph",
        "parameters": [
          {
            "description": "If set to true with neocna set to true as well, distributed flooding will be used only and only if the feature is enabled on the platform.",
            "in": "query",
            "name": "floodrpc",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "description": "If set to true, neocna will be used regardless of whether tenant is set to use neocna.",
            "in": "query",
            "name": "neocna",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/cloudgraph"
              }
            }
          }
        },

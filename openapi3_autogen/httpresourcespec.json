      "get": {
        "description": "Retrieves the list of HTTP resource specifications.",
        "operationId": "get-all-httpresourcespecs",
        "parameters": [
          {
            "description": "Also retrieve the objects that have been archived.",
            "in": "query",
            "name": "archived",

    "/accessreports": {
      "post": {
        "description": "Create an access report.",
        "operationId": "create-a-new-accessreport",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/accessreport"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/accessreport"
                }
              }
            },
            "description": "n/a"
          }
        },
        "tags": [
          "policy/access",
          "zack"
        ]
      }
    }
  },
  "tags": [

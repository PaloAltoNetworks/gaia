        "operationId": "create-a-new-cloudgraphasset",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/cloudgraphasset"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {

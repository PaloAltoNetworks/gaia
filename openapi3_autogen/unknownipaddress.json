    "schemas": {
      "unknownipaddress": {
        "description": "Holds the IP/FQDN of flows going to somewhere (default) external network.",
        "properties": {
          "ID": {
            "description": "Identifier of the object.",
            "readOnly": true,
            "type": "string"
          },
          "address": {
            "description": "IP/FQDN encountered.",
            "example": "google.com",
            "type": "string"
          },
          "annotations": {
            "additionalProperties": {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "description": "Stores additional information about an entity.",
            "type": "object"
          },
          "associatedTags": {
            "description": "List of tags attached to an entity.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "createTime": {
            "description": "Creation date of the object.",
            "format": "date-time",
            "readOnly": true,
            "type": "string"
          },
          "namespace": {
            "description": "Namespace tag attached to an entity.",
            "readOnly": true,
            "type": "string"
          },
          "normalizedTags": {
            "description": "Contains the list of normalized tags of the entities.",
            "items": {
              "type": "string"
            },
            "readOnly": true,
            "type": "array"
          },
          "occurrences": {
            "description": "Number of times the address was seen for the source.",
            "type": "integer"
          },
          "protected": {
            "description": "Defines if the object is protected.",
            "type": "boolean"
          },
          "sourceID": {
            "description": "Identifier of the source of the address.",
            "example": "62aa2888a76fe8dc9efa1000",
            "type": "string"
          },
          "sourceIdentity": {
            "description": "Identity of the source of the address.",
            "example": "flowreport",
            "type": "string"
          },
          "sourceNamespace": {
            "description": "Namespace of the source of the address.",
            "example": "/my/namespace",
            "type": "string"
          },
          "timestamp": {
            "description": "The timestamp of the source of the address.",
            "example": "2018-06-14T23:10:46.420397985Z",
            "format": "date-time",
            "type": "string"
          },
          "updateTime": {
            "description": "Last update date of the object.",
            "format": "date-time",
            "readOnly": true,
            "type": "string"
          }
        },
        "required": [
          "address",
          "sourceID",
          "sourceIdentity",
          "sourceNamespace",
          "timestamp"
        ],
        "type": "object"
      }

  "components": {
    "schemas": {
      "suggestedpolicy": {
        "description": "Allows you to obtain network ruleset policy suggestions.",
        "properties": {
          "policy": {
            "$ref": "./networkrulesetpolicy#/components/schemas/networkrulesetpolicy"
          }
        },
        "type": "object"
      }
    }
  },
  "info": {
    "contact": {

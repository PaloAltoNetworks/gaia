{
  "openapi": "3.0.3",
  "components": {
    "schemas": {
      "revocation": {
        "description": "Used to revoke a certificate.",
        "properties": {
          "expirationDate": {
            "description": "Contains the certificate expiration date. This will be used to clean up revoked\ncertificates that have expired.",
            "format": "date-time",
            "type": "string"
          },
          "revokeDate": {
            "description": "Set time from when the certificate will be revoked.",
            "format": "date-time",
            "type": "string"
          },
          "serialNumber": {
            "description": "SerialNumber of the revoked certificate.",
            "type": "string"
          },
          "subject": {
            "description": "Subject of the certificate related to the revocation.",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },

{
  "openapi": "3.0.3",
  "components": {
    "schemas": {
      "bucketdestinationport": {
        "description": "Holds the destination ports of flows across 24 hours (beginning of a day to\nthe end).",
        "properties": {
          "ID": {
            "description": "Identifier of the object.",
            "readOnly": true,
            "type": "string"
          },
          "date": {
            "description": "The date that the bucket tracks.",
            "example": "2022-05-09",
            "type": "string"
          },
          "namespace": {
            "description": "Namespace tag attached to an entity.",
            "readOnly": true,
            "type": "string"
          },
          "ports": {
            "additionalProperties": {
              "items": {
                "type": "object"
              },
              "type": "array"
            },
            "description": "Ports encountered and number of occurrences seen.",
            "type": "object"
          },
          "topPorts": {
            "description": "List of most encountered ports, sorted by occurrences.",
            "items": {
              "$ref": "./buckettopentry#/components/schemas/buckettopentry"
            },
            "readOnly": true,
            "type": "array"
          }
        },
        "required": [
          "date"
        ],
        "type": "object"
      }
    }
  },

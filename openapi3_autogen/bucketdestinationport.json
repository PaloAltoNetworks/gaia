        "description": "Retrieves the list of destination port buckets.",
        "operationId": "get-all-bucketdestinationports",
        "parameters": [
          {
            "description": "if set, it will return the top entries up to the number specified (default 10).",
            "in": "query",
            "name": "numberOfTopEntries",
            "schema": {
              "type": "integer"
            }
          },
          {
            "description": "Filtering query. Consequent `q` parameters will form an or.",
            "example": "associatedTags contains ['$name=hello'] or type == 'Docker'",
            "in": "query",
            "name": "q",

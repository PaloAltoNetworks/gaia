# Attributes
attributes:
  v1:
  - name: clusterID
    description: The ID of the cloud cluster the resource belongs.
    type: string
    exposed: true
    stored: true
    required: true
    example_value: https://container.googleapis.com/v1/projects/my-project/zones/us-central1-c/clusters/my-cluster

  - name: data
    description: The JSON-encoded data that represents the resource.
    type: external
    exposed: true
    subtype: '[]byte'
    stored: true
    required: true
    example_value: |-
      {
        "apiVersion": "v1",
        "kind": "Service",
        ...
      }

  - name: denormedFields
    description: Contextual values that can be used to narrow searching of resources.
    type: list
    exposed: true
    subtype: string
    stored: true
    read_only: true
    autogenerated: true

  - name: k8sNamespace
    description: The k8s Namespace of the resource.
    type: string
    exposed: true
    stored: true
    required: true
    example_value: my-namespace

  - name: kind
    description: The specific kind of the k8s resource.
    type: enum
    exposed: true
    stored: true
    read_only: true
    allowed_choices:
    - Pending
    - Pod
    - Deployment
    - ReplicaSet
    - StatefulSet
    - DaemonSet
    - Namespace
    - NetworkPolicy
    - Cluster
    - Service
    - EndpointSlice
    autogenerated: true
    default_value: Pending
    example_value: Service

  - name: labels
    description: Key value label pairs for k8s resources.
    type: external
    exposed: true
    subtype: map[string]string
    read_only: true
    omit_empty: true

  - name: name
    description: The name of the resource.
    type: string
    exposed: true
    stored: true
    example_value: abc

  - name: prismaUnifiedAssetID
    description: The Prisma Cloud Unified Asset ID.
    type: string
    exposed: true
    stored: true
    example_value: 4b882952f1a26c8a3ce16ee90af3cac9

  - name: uid
    description: The k8s UID of the resource.
    type: string
    exposed: true
    stored: true
    example_value: abc

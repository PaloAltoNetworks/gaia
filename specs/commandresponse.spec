# Model
model:
  rest_name: commandresponse
  resource_name: commandresponse
  entity_name: CommandResponse
  package: guy
  group: core/enforcer
  description: A result from a command request.
  extends:
  - '@namespaced'

# Attributes
attributes:
  v1:
  - name: base64Encoded
    description: Is the response base64 encoded.
    type: boolean
    exposed: true

  - name: commandID
    description: Unique ID generated for each command.
    type: string
    exposed: true
    read_only: true
    autogenerated: true

  - name: compressed
    description: Is the response compressed.
    type: boolean
    exposed: true

  - name: response
    description: The response data.
    type: string
    exposed: true

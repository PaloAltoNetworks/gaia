# Model
model:
  rest_name: statsinfo
  resource_name: statsinfo
  entity_name: StatsInfo
  package: jenova
  group: visualization/statsquery
  description: Lists the fields and tags available in a statistics measurement.
  aliases:
  - si

# Attributes
attributes:
  v1:
  - name: fields
    description: Contains the list of fields. You cannot group by these fields.
    type: external
    exposed: true
    subtype: map[string]string
    read_only: true
    autogenerated: true

  - name: measurement
    description: Name of the measurement.
    type: enum
    exposed: true
    allowed_choices:
    - Flows
    - Audit
    - Enforcers
    - Files
    - EventLogs
    - Accesses
    default_value: Flows

  - name: tags
    description: Contains the list of tags. You can group by these tags.
    type: list
    exposed: true
    subtype: string
    read_only: true
    autogenerated: true

# Attributes
attributes:
  v1:
  - name: zHash
    description: |-
      geographical hash of the data. This is used for sharding and
      georedundancy.
    type: integer
    stored: true
    read_only: true
    autogenerated: true
    getter: true
    setter: true

  - name: zone
    description: |-
      geographical zone. This is used for sharding and
      georedundancy.
    type: integer
    stored: true
    read_only: true
    autogenerated: true
    getter: true
    setter: true

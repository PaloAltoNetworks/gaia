# Attributes
attributes:
  v1:
  - name: realm
    description: Realm is the authentication realm.
    type: enum
    exposed: true
    required: true
    allowed_choices:
    - AWSIdentityDocument
    - Certificate
    - Facebook
    - Github
    - Google
    - LDAP
    - Twitter
    - Vince
    - GCPIdentityDocument
    - AzureIdentityDocument
    example_value: Certificate

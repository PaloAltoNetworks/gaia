# Model
model:
  rest_name: reputation
  resource_name: reputations
  entity_name: Reputation
  package: karl
  group: pc/pandb
  description: Represents the reputation information from PANDB.
  aliases:
  - rep

# Attributes
attributes:
  v1:
  - name: URLReputations
    description: List of URL reputations.
    type: refList
    exposed: true
    subtype: urlreputation
    read_only: true
    autogenerated: true
    omit_empty: true
    extensions:
      refMode: pointer

  - name: URLs
    description: The evidence related to the risk level.
    type: list
    exposed: true
    subtype: string
    omit_empty: true
